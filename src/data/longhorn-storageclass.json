{
  "kind": "StorageClass",
  "apiVersion": "storage.k8s.io/v1",
  "metadata": {
    "name": "longhorn",
    "annotations": { "storageclass.kubernetes.io/is-default-class": "true" }
  },
  "provisioner": "rancher.io/longhorn",
  "parameters": {
    "numberOfReplicas": "3",
    "staleReplicaTimeout": "30",
    "fromBackup": ""
  }
}
